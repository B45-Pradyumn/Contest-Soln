import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Tour_of_king
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int p = sc.nextInt();
		for(int q=0; q<p; q++){
		    int r = sc.nextInt();
		    int s = sc.nextInt();
		    System.out.println((r*5)+(s*7));
		}
	}
}
