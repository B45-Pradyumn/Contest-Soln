 string printString(string s, char ch, int count) {
        int occ=0;
        for(int i=0;i<s.length();i++ )
        {
            if(s[i]==ch)
            {
                occ++;
            }
            if(occ==count)
            {
                return s.substr(i+1);
            }
        }
        return "";
    }

