class Solution {
    static int matchGame(Long N) {
     /* N         Number of Matchsticks 
        1          1
        2          2
        3          3 
        4          4
        5          -1   
        6          1 
        7           2
        8           3
        9           4
        10         -1
        11          1 
        12....................
     */
        long res=N%5;
        
        if(res!=0)
        return (int)res;
        else
        return -1;
    }
};
