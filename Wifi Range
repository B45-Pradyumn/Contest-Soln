class Solution 
{ 
    boolean wifiRange(int N, String S, int X) 
    { 
        int count = 0;
        int i = 0;
        while(i<N){
            if(S.charAt(i)=='0')
                count++;
            else
                count = (-1)*(X);
            i++;
            if(count>X)
                return false;
        }
        if(count>0)
            return false;
        return true;
    }
} 
